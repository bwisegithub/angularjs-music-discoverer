{"sparqlQueries":[
	{"id"		:	"1", 
	 "name"		:	"keywordSearchQuery",
	 "query"	:	[
					"SELECT",
					"  ?id",
					"  STR(?label) AS ?label",
					"  ?thumbnail",
					"WHERE",
					"{",
					"  { ?resource a dbpedia-owl:MusicalArtist }",
					"      UNION",
					"  { ?resource a dbpedia-owl:Band }",
					"  ?resource dbpedia-owl:wikiPageID ?id .",
					"  ?resource rdfs:label ?label .",
					"  ?resource rdfs:comment ?comment .",
					"  OPTIONAL{ ?resource dbpedia-owl:thumbnail ?thumbnail }",
					"  FILTER(langMatches(lang(?label),\"EN\"))",
					"  FILTER(langMatches(lang(?comment),\"EN\"))",
					"  FILTER(!regex(?label, \"List of\", \"i\"))",
					"  FILTER(bif:contains(?label,\"--REPLACE_KEYWORDS--\"))",
					"}  ",
					"ORDER BY ?label",
					"LIMIT 100"
					]
	},
	{"id"		:	"2", 
	 "name"		:	"randomIdQuery",
	 "query"	:	[
					"SELECT",
					"  ?id",
					"WHERE ",
					"{",
					"  { ?resource a dbpedia-owl:MusicalArtist }",
					"      UNION",
					"  { ?resource a dbpedia-owl:Band }",
					"  ?resource dbpedia-owl:wikiPageID ?id .",
					"  ?resource rdfs:label ?label .",
					"  ?resource rdfs:comment ?comment",
					"  FILTER(langMatches(lang(?label),\"EN\"))",
					"  FILTER(langMatches(lang(?comment),\"EN\"))",
					"  FILTER(!regex(?label, \"List of\", \"i\"))",
					"}",
					"OFFSET --REPLACE_OFFSET--",
					"LIMIT 1"
					]
	},
	{"id"		:	"3", 
	 "name"		:	"countForGenreQuery",
	 "query"	:	[
					"SELECT",
					"  count(?resource) AS ?resourceCnt",
					"WHERE",
					"{",
					"  { ?resource a dbpedia-owl:MusicalArtist }",
					"      UNION",
					"  { ?resource a dbpedia-owl:Band }",
					"  ?resource rdfs:label ?label .",
					"  ?resource rdfs:comment ?comment",
					"  FILTER(langMatches(lang(?label),\"EN\"))",
					"  FILTER(langMatches(lang(?comment),\"EN\"))",
					"  FILTER(!regex(?label, \"List of\", \"i\"))",
					"  FILTER EXISTS{ ?resource dbpedia-owl:genre <--REPLACE_GENRE_RESOURCE--> }",
					"} "
					]
	},
	{"id"		:	"4", 
	 "name"		:	"randomIdForGenreQuery",
	 "query"	:	[
					"SELECT",
					"  ?id",
					"WHERE",
					"{",
					"  { ?resource a dbpedia-owl:MusicalArtist }",
					"      UNION",
					"  { ?resource a dbpedia-owl:Band }",
					"  ?resource dbpedia-owl:wikiPageID ?id .",
					"  ?resource rdfs:label ?label .",
					"  ?resource rdfs:comment ?comment",
					"  FILTER(langMatches(lang(?label),\"EN\"))",
					"  FILTER(langMatches(lang(?comment),\"EN\"))",
					"  FILTER(!regex(?label, \"List of\", \"i\"))",
					"  FILTER EXISTS{ ?resource dbpedia-owl:genre <--REPLACE_GENRE_RESOURCE--> }",
					"} ",
					"OFFSET --REPLACE_OFFSET--",
					"LIMIT 1"
					]
	},
	{"id"		:	"5", 
	 "name"		:	"musicianDetailsQuery",
	 "query"	:	[
					"SELECT",
					"  ?resource",
					"  (STR(?resource_label) AS ?resource_label)",
					"  ?thumbnail",
					"  REPLACE( STR(?thumbnail), \"Special:FilePath/\", \"File:\" ) as ?thumbnail_wm_link",
					"  (STR(?comment) AS ?comment)",
					"  ?genre_resource",
					"  (STR(?genre_label) AS ?genre_label)",
					"{",
					"  ?resource dbpedia-owl:wikiPageID --REPLACE_ID-- .",
					"  ?resource rdfs:label ?resource_label .",
					"  ?resource rdfs:comment ?comment .",
					"  OPTIONAL{ ?resource dbpedia-owl:thumbnail ?thumbnail } .",
					"  OPTIONAL{ ?resource dbpedia-owl:genre ?genre_resource .",
					"            ?genre_resource rdfs:label ?genre_label .",
					"             FILTER(langMatches(lang(?genre_label),\"EN\"))",
					"           }",
					"  FILTER(langMatches(lang(?resource_label),\"EN\"))",
					"  FILTER(langMatches(lang(?comment),\"EN\"))",
					"}"
					]
	},
	{"id"		:	"6", 
	 "name"		:	"musicianAssociatedQuery",
	 "query"	:	[
					"SELECT",
					"  ?id",
					"  (STR(?label) AS ?label)",
					"  ?thumbnail",
					"  REPLACE( GROUP_CONCAT(DISTINCT CONCAT(?assoc_subject_id, \":\", ?assoc_subject_label); separator='|') , \"^:+|:+$\", \"\" ) as ?assoc_subject_arr",
					"  REPLACE( GROUP_CONCAT(DISTINCT CONCAT(?assoc_object_id, \":\", ?assoc_object_label); separator='|') , \"^:+|:+$\", \"\" ) as ?assoc_object_arr",
					"{",
					"  { ?resource dbpedia-owl:wikiPageID --REPLACE_ID-- }",
					"    UNION",
					"  { <--REPLACE_RESOURCE--> dbpedia-owl:associatedMusicalArtist ?resource }",
					"    UNION",
					"  { ?resource dbpedia-owl:associatedMusicalArtist <--REPLACE_RESOURCE-->}",
					"  ?resource dbpedia-owl:wikiPageID ?id .",
					"  ?resource rdfs:label ?label .",
					"  ?resource rdfs:comment ?comment .",
					"  OPTIONAL{ ?resource dbpedia-owl:thumbnail ?thumbnail } .",
					"  OPTIONAL{ ?resource dbpedia-owl:associatedMusicalArtist ?assoc_object . ",
					"            ?assoc_object dbpedia-owl:wikiPageID ?assoc_object_id  .",
					"            ?assoc_object rdfs:label ?assoc_object_label .",
					"            ?assoc_object rdfs:comment ?assoc_object_comment",
					"            FILTER(langMatches(lang(?assoc_object_label),\"EN\"))",
					"            FILTER(langMatches(lang(?assoc_object_comment),\"EN\"))",
					"          } .",
					"  OPTIONAL{ ?assoc_subject dbpedia-owl:associatedMusicalArtist ?resource .",
					"            ?assoc_subject dbpedia-owl:wikiPageID ?assoc_subject_id .",
					"            ?assoc_subject rdfs:label ?assoc_subject_label .",
					"            ?assoc_subject rdfs:comment ?assoc_subject_comment",
					"            FILTER(langMatches(lang(?assoc_subject_label),\"EN\"))",
					"            FILTER(langMatches(lang(?assoc_subject_comment),\"EN\"))",
					"          }",
					"  FILTER(langMatches(lang(?label),\"EN\"))",
					"  FILTER(langMatches(lang(?comment),\"EN\"))",
					"  FILTER(!regex(?label, \"List of\", \"i\"))",
					"}",
					"LIMIT 100"
					]
	}	
]}
